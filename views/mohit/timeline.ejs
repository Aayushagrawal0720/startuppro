<div class="process-wrapper">
  <div id="progress-bar-container">
    <ul>
      <li class="step step01 active">
        <div class="step-inner">
          <img src=<%=foundData[0].picUrl %> alt="" checked class="img-cla" >
          <br >
          <br >
          <br >
          <%= foundData[0].event_title %>
        </div>
      </li>

      <% foundData.forEach(function(item, index) { %>
        <% if (index> 0 && index < 5) { %>
            <li class="step step0<%= index + 1 %>">
              <div class="step-inner">
                <img src=<%=foundData[index].picUrl %>
                alt=""
                checked
                class="img-cla"
                >
                <br>
                <br>
                <br>
                <%= foundData[index].event_title %>
              </div>
            </li>
            <% } %>
              <% }); %>


    </ul>

    <div id="line">
      <div id="line-progress"></div>
    </div>
  </div>

  <div id="progress-content-section">
    <div class="section-content discovery active">
      <h2>
        <%= foundData[0].event_title %>
      </h2>
      <p>
        <%= foundData[0].description %>
      </p>
    </div>

    <% foundData.forEach(function(item, index) { %>
      <% if (index>=0 && index < 5) { %>
          <div class="section-content desc0<%= index + 1 %>">
            <h2>
              <%= foundData[index].event_title %>
            </h2>
            <p>
              <%= foundData[index].description %>
            </p>
          </div>
          <% } %>
            <% }); %>
  </div>
</div>

